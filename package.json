{
	"name": "duo-admin",
	"version": "1.0.0",
	"private": true,
	"description": "DUO Admin",
	"author": "FinBook Pte Ltd",
	"scripts": {
		"test": "jest",
		"tslint": "tslint -c tslint.json -p tsconfig.json src/**/*.ts{,x} --fix",
		"commit": "ts-node ./src/index commit",
		"bitfinex": "ts-node ./src/index bitfinex",
		"gemini": "ts-node ./src/index gemini",
		"kraken": "ts-node ./src/index kraken",
		"gdax": "ts-node ./src/index gdax",
		"subscribe": "ts-node ./src/index subscribe",
		"minutely": "ts-node ./src/index minutely dynamo",
		"hourly": "ts-node ./src/index hourly dynamo"
	},
	"jest": {
		"roots": [
			"<rootDir>/src"
		],
		"transform": {
			"^.+\\.tsx?$": "ts-jest"
		},
		"testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"jsx",
			"json",
			"node"
		]
	},
	"devDependencies": {
		"@types/jest": "^22.2.3",
		"@types/mysql": "^2.15.4",
		"@types/request": "^2.47.0",
		"@types/ws": "^4.0.2",
		"jest": "^22.4.3",
		"ts-jest": "^22.4.4",
		"tslint": "^5.9.1"
	},
	"dependencies": {
		"abi-decoder": "^1.1.0",
		"aws-sdk": "^2.245.1",
		"ethereumjs-tx": "^1.3.4",
		"moment": "^2.21.0",
		"mysql": "^2.15.0",
		"node-schedule": "^1.3.0",
		"request": "^2.85.0",
		"stochastic": "0.0.14",
		"ts-node": "^6.0.0",
		"typescript": "^2.8.3",
		"web3": "^1.0.0-beta.33",
		"ws": "^5.1.1"
	}
}
