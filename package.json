{
	"name": "duo-pricefeed",
	"version": "1.0.0",
	"private": true,
	"description": "DUO Price Feed",
	"author": "FinBook Pte Ltd",
	"scripts": {
		"test": "jest",
		"tslint": "tslint -c tslint.json -p tsconfig.json",
		"readContract": "ts-node ./src/index readContract",
		"decoder": "ts-node ./src/index decoder",
		"calculatePrice": "ts-node ./src/index calculatePrice",
		"pf": "ts-node ./src/index pf",
		"acceptPrice": "ts-node ./src/index acceptPrice",
		"createAccount": "ts-node ./src/index createAccount",
		"removeAccount": "ts-node ./src/index removeAccount",
		"allAccounts": "ts-node ./src/index allAccounts",
		"bitfinex": "ts-node ./src/index bitfinex",
		"gemini": "ts-node ./src/index gemini",
		"kraken": "ts-node ./src/index kraken",
		"gdax": "ts-node ./src/index gdax",
		"acceptPriceEvent": "ts-node ./src/index acceptPriceEvent",
		"preResetEvent": "ts-node ./src/index preResetEvent",
		"resetEvent": "ts-node ./src/index resetEvent",
		"create": "ts-node ./src/index create"
	},
	"jest": {
		"roots": [
			"<rootDir>/src"
		],
		"transform": {
			"^.+\\.tsx?$": "ts-jest"
		},
		"testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"jsx",
			"json",
			"node"
		]
	},
	"devDependencies": {
		"@types/jest": "^22.2.3",
		"@types/mysql": "^2.15.4",
		"@types/request": "^2.47.0",
		"@types/ws": "^4.0.2",
		"jest": "^22.4.3",
		"ts-jest": "^22.4.4",
		"tslint": "^5.9.1"
	},
	"dependencies": {
		"abi-decoder": "^1.1.0",
		"ethereumjs-tx": "^1.3.4",
		"moment": "^2.21.0",
		"mysql": "^2.15.0",
		"node-schedule": "^1.3.0",
		"request": "^2.85.0",
		"ts-node": "^6.0.0",
		"typescript": "^2.8.3",
		"web3": "^1.0.0-beta.33",
		"ws": "^5.1.1"
	}
}
