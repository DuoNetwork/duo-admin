{
	"name": "duo-admin",
	"version": "1.0.0",
	"private": true,
	"description": "DUO Admin",
	"author": "FinBook Pte Ltd",
	"scripts": {
		"test": "jest",
		"tslint": "tslint -c tslint.json -p tsconfig.json src/**/*.ts{,x} --fix",
		"commit": "ts-node ./src/index commit",
		"bitfinex": "ts-node ./src/index bitfinex",
		"gemini": "ts-node ./src/index gemini",
		"kraken": "ts-node ./src/index kraken",
		"gdax": "ts-node ./src/index gdax",
		"subscribe": "ts-node ./src/index subscribe",
		"minutely": "ts-node ./src/index minutely dynamo",
		"hourly": "ts-node ./src/index hourly dynamo",
		"node": "ts-node ./src/index node"
	},
	"jest": {
		"roots": [
			"<rootDir>/src"
		],
		"transform": {
			"^.+\\.tsx?$": "ts-jest"
		},
		"testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"jsx",
			"json",
			"node"
		]
	},
	"devDependencies": {
		"@types/jest": "^23.0.0",
		"@types/mysql": "^2.15.5",
		"@types/request": "^2.47.0",
		"@types/ws": "^5.1.1",
		"jest": "^23.1.0",
		"ts-jest": "^22.4.6",
		"tslint": "^5.10.0"
	},
	"dependencies": {
		"@google-cloud/storage": "^1.7.0",
		"abi-decoder": "^1.1.0",
		"aws-sdk": "^2.252.1",
		"ethereumjs-tx": "^1.3.4",
		"moment": "^2.22.2",
		"mysql": "^2.15.0",
		"node-schedule": "^1.3.0",
		"request": "^2.87.0",
		"stochastic": "0.0.14",
		"ts-node": "^6.1.0",
		"typescript": "^2.9.1",
		"web3": "^1.0.0-beta.34",
		"ws": "^5.2.0"
	}
}
