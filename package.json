{
	"name": "duo-admin",
	"version": "1.0.0",
	"private": true,
	"description": "DUO Admin",
	"author": "FinBook Pte Ltd",
	"scripts": {
		"test": "jest",
		"build": "tsc",
		"prepare": "npm run build",
		"tslint": "tslint -c tslint.json -p tsconfig.json src/**/*.ts{,x} --fix",
		"commit": "ts-node ./src/index commit",
		"trades": "ts-node src/index trades",
		"prices": "ts-node src/index prices",
		"node": "ts-node ./src/index node",
		"cleanDB": "ts-node ./src/index cleanDB",
		"fetchPrice": "ts-node ./src/index fetchPrice",
		"startCustodian": "ts-node ./src/index startCustodian",
		"round": "ts-node ./src/index round",
		"events": "ts-node ./src/index events"
	},
	"jest": {
		"roots": [
			"<rootDir>/src"
		],
		"transform": {
			"^.+\\.tsx?$": "ts-jest"
		},
		"testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"jsx",
			"json",
			"node"
		]
	},
	"devDependencies": {
		"@types/jest": "^24.0.17",
		"@types/mysql": "^2.15.6",
		"@types/pusher-js": "^4.2.2",
		"@types/request": "^2.48.2",
		"@types/ws": "^6.0.2",
		"coveralls": "^3.0.6",
		"jest": "^24.8.0",
		"overalls": "^1.0.4",
		"pre-commit": "^1.2.2",
		"ts-jest": "^24.0.2",
		"tslint": "^5.18.0"
	},
	"dependencies": {
		"@babel/polyfill": "^7.4.4",
		"@finbook/duo-contract-wrapper": "^1.5.0",
		"@finbook/duo-market-data": "^1.0.6",
		"@google-cloud/storage": "^3.1.0",
		"aws-cli-js": "^2.0.6",
		"bitfinex-api-node": "1.2.1",
		"moment": "^2.24.0",
		"mysql": "^2.17.1",
		"node-schedule": "^1.3.2",
		"pusher-js": "^5.0.0",
		"request": "^2.88.0",
		"ts-node": "^8.3.0",
		"typescript": "^3.5.3",
		"ws": "^7.1.2"
	}
}
